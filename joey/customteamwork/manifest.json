{
    "manifest_version": 3,
    "name": "Teamwork Custom CSS",
    "version": "1.0",
    "description": "Custom CSS for Teamwork",
    "permissions": ["activeTab", "tabs"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://getuwiredinc.teamwork.com/*"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ]
  }